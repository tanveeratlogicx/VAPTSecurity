{
    "name": "vapt-security/vapt-security"
    "description": "A comprehensive WordPress security plugin that protects against common VAPT issues including DoS attacks via wp-cron, lack of input validation, and rate limiting on form submissions.",
    "type": "wordpress-plugin",
    "keywords": ["wordpress", "security", "vapt", "dos-protection", "input-validation", "rate-limiting"],
    "homepage": "https://github.com/your-username/vapt-security"
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Security Qoder",
            "email": "security@example.com",
            "homepage": "https://github.com/your-username",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/your-username/vapt-security/issues"
        "source": "https://github.com/your-username/vapt-security"
    },
    "require": {
        "php": ">=7.2.24"
    },
    "suggest": {
        "ext-mbstring": "For better multibyte string handling"
    },
    "autoload": {
        "classmap": ["includes/"]
    },
    "config": {
        "platform": {
            "php": "7.2.24"
        }
    }
}